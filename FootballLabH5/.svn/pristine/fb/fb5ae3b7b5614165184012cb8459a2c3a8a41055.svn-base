    /* tabcss */
    
    .slideTxtBox {
        width: 100%;
        text-align: left;
    }
    
    .slideTxtBox .list-header {
        background: #35528c;
        width: 100%;
        margin-bottom: 1px;
        margin-top: 5px;
    }
    
    .slideTxtBox .list-header ul {
    	display: table;
        width: 100%;
    }
    
    .slideTxtBox .list-header ul li {   
        height: 40px;
        display: table-cell;
        vertical-align: middle;
        cursor: pointer;
        text-align: center;
        font-size: 1.25rem;
    }
    .slideTxtBox .list-header ul li:nth-child(1){
    	background: RGB(204,172,67);
    }     
    .slideTxtBox .list-header ul li:nth-child(2){
    	border-left: 1px solid RGB(48,63,96);
    	border-right: 1px solid RGB(48,63,96);
    	background: RGB(88,185,204);
    } 
    .slideTxtBox .list-header ul li:nth-child(3){
    	background: RGB(88,204,154);
    }        
    .slideTxtBox .list-header ul li span {
    	color: #333;
        font-size: 1.25rem;
        display: inline-block;
        width: 100%;
    }
    
    .noborder {
        border-right-width: 0px!important;
    }
    
    .slideTxtBox .list-body {
    	display: table;
    }
    .slideTxtBox .list-body ul {
    	display: table-cell;
        zoom: 1;
    }
    
    .slideTxtBox .list-body li {
        width: 100%;
        padding: 10px;
        background: #35528c;
        margin-bottom: 1px;
        position: relative;
    }
    
    /*.slideTxtBox .bd li:nth-child(odd) {
        margin-right: 1px;
        margin-left: -1px;
    }*/
    
    .slideTxtBox .list-body li .date {
        float: right;
        color: #999;
    }
    /* 下面是前/后按钮代码，如果不需要删除即可 */
    
    .slideTxtBox .arrow {
        position: absolute;
        right: 10px;
        top: 0;
    }
    
    .slideTxtBox .arrow a {
        display: block;
        width: 5px;
        height: 9px;
        float: right;
        margin-right: 5px;
        margin-top: 10px;
        overflow: hidden;
        cursor: pointer;
        background: url("../images/arrow.png") 0 0 no-repeat;
    }
    
    .slideTxtBox .arrow .next {
        background-position: 0 -50px;
    }
    
    .slideTxtBox .arrow .prevStop {
        background-position: -60px 0;
    }
    
    .slideTxtBox .arrow .nextStop {
        background-position: -60px -50px;
    }
    
