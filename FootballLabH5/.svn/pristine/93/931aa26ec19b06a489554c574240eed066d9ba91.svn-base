/*提示框css*/
#lss_tishi_error{position: fixed;height: auto;max-width:250px;border-radius: 4px;z-index: 999;top: 100px;left: 100px;display: none;}
#lss_tishi_error .box{position: absolute;width: 100%;height: 100%;background: #000;opacity: 0.7;border-radius: 4px;z-index: 1;}
#lss_tishi_error .con{line-height: 30px;color: #fff;position:relative; top:0px;left: 0px;z-index: 8888;padding: 5px 10px;}

/*alert提示错误*/
#lss_alert_error{width: 280px;height:auto;background: #fff;border-radius: 5px;position: fixed;z-index: 1000;display: none;border:1px solid #ddd;-moz-box-shadow: 2px 2px 10px #909090;/*firefox*/-webkit-box-shadow: 2px 2px 10px #909090;/*safari或chrome*/box-shadow:2px 2px 10px #909090;/*opera或ie9*/}
#lss_alert_error .con{width: 100%;line-height: 25px;border-bottom: 1px solid #dddddd;text-align: center;color: #000;padding: 10px;}
#lss_alert_error .clone{width: 100%;height: 35px;line-height: 35px;text-align: center;color: #333333;float: left;display: block;color: #029B75;}

/*confirm提示错误*/
#lss_confirm_error{width: 280px;height:auto;background: #fff;border-radius: 5px;position: fixed;z-index: 1001;display: none;border:1px solid #ddd;-moz-box-shadow: 2px 2px 10px #909090;/*firefox*/-webkit-box-shadow: 2px 2px 10px #909090;/*safari或chrome*/box-shadow:2px 2px 10px #909090;/*opera或ie9*/}
#lss_confirm_error .con{width: 100%;line-height: 25px;border-bottom: 1px solid #dddddd;text-align: center;color: #000;padding: 10px;}
#lss_confirm_error .confirm{width: 140px;height: 35px;line-height: 35px;text-align: center;color: #333333;float: left;display: block;border-right: 1px solid #ddd;color: #029B75;}
#lss_confirm_error .cancel{width: 137px;height: 35px;line-height: 35px;text-align: center;color: #333333;float: left;display: block;color: #029B75;}

/*loading提示*/
#lss_loading{position: fixed;top: 50%;left: 50%;background: #fff;z-index: 1002;border-radius: 10px;display: none;text-align: center;padding: 10px;}
#lss_loading>img{width: 40px;height: 40px;}
#lss_loading>span{text-align: center;line-height: 20px;display:inline-block;float: left;font-size: 13px;color: #000; }

/*黑色背景*/
#lss_black_bg{width: 100%;height: 100%;position: fixed;top: 0px;left: 0px;background: #000;opacity: 0.5;z-index: 998;display: none;}
#lss_black_bg1{width: 100%;height: 100%;position: fixed;top: 0px;left: 0px;background: #000;opacity: 0.5;z-index: 998;display: none;}
/*提示框结束*/
