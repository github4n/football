body,
html {
    margin: 0;
    padding: 0;
}
/*盈利走势图css */
a:hover{
	text-decoration: none;
	color: #333;
}
#owl-demo .item img {
    display: block;
    width: 100%;
    height: auto;
}
.mybtn{
	width: 46%;
	margin: 0 2%;
	text-align: center;
	height: 30px;
	line-height: 30px;
	font-size: 1.2rem;
	border-radius: 5px;
}
.bt_line{
	border-bottom: 1px solid rgb(236,233,226);
}
.btn_box{
	margin: 10px 0;
}
.btn_box a{
	float: left;
	display: block;
	
	
}
.subNav-t {
    color: #ddd;
    line-height: 34px;
    text-align: center;
    float: left;
    width: 50%;
    box-sizing: border-box;
}

.align_left{
	text-align: left;
}
.align_right{
	text-align: right;
}
.subMenu-t{
	padding:0 10px;
}
